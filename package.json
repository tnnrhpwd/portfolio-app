{
  "name": "sthopwood",
  "version": "1.0.0",
  "author": "Steven Tanner Hopwood <steven.t.hopwood@gmail.com>",
  "scripts": {
    "start": "concurrently \"npm start --prefix frontend\" \"npm run server --prefix backend\"",
    "dev": "concurrently \"npm start --prefix frontend\" \"npm run dev --prefix backend\"",
    "build": "npm run build --prefix frontend && npm run build --prefix backend",
    "test": "jest --watchAll --verbose",
    "test:coverage": "jest --coverage --watchAll=false",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "install-all": "npm install && concurrently \"npm install --prefix frontend\" \"npm install --prefix backend\"",
    "build:netlify": "npm install --prefix frontend && npm run build --prefix frontend",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "health-check": "node scripts/health-check.js"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.14.9",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(svg|less)$": "identity-obj-proxy",
      "\\.(png|less)$": "<rootDir>/frontend/src/assets/__mocks__/mock.js"
    },
    "testEnvironment": "jest-environment-jsdom"
  },
  "overrides": {
    "semver": "^7.6.0"
  },
  "dependencies": {
    "@babel/preset-react": "^7.22.15",
    "@stripe/stripe-js": "^5.5.0",
    "concurrently": "^7.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-localstorage-mock": "^2.4.26",
    "node-cache": "^5.1.2",
    "text-encoding": "^0.7.0"
  }
}
